{
  "compilerOptions": {
    "target": "ES6",
    "lib": ["ES6", "DOM"],
    "module": "CommonJS",
    "strict": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "types": ["cypress", "node"]
  },
  "include": ["cypress/**/*.ts",
"node_modules/cypress/**/*.ts",
"cypress/integration/**/*.ts"
]
}